{
    "compilerOptions": {
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "resolveJsonModule": true,
      "module": "commonjs",
      "target": "es6",
      "allowJs": false,
      "sourceMap": true,
      "outDir": "dist",
      "strictNullChecks": true,
      "esModuleInterop": true,
      "noImplicitAny": true,
      "noUnusedLocals": false,
      "noUnusedParameters": false
    },
    "exclude": ["node_modules", "dist", "**.js"]
  }
  