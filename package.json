{
  "name": "app-service-api",
  "version": "1.0.0",
  "description": "app-service-api",
  "main": "index.js",
  "engines": {
    "node": "^18.0.0",
    "npm": "^9.5.0"
  },
  "scripts": {
    "test": "mocha --require ts-node/register  \"./test/**/*.ts\" --exit --timeout 15000",
    "test:coverage": "nyc mocha --require ts-node/register  \"./test/**/*.ts\" --exit --timeout 15000",
    "test:report": "nyc report --reporter=cobertura",
    "start": "node dist/src/server.js",
    "start:dev": "node ./dist/src/server.js",
    "restart:dev": "tsc && node ./dist/src/server.js",
    "lint": "eslint **/*.ts",
    "lint-fix": "eslint **/*.ts --fix",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node-dev --respawn --transpile-only --watch src -r dotenv/config src/server.ts",
    "format": "pretty-quick"
  },
  "nyc": {
    "lines": 80,
    "check-coverage": true,
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-jaeger": "^1.28.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.57.0",
    "@opentelemetry/instrumentation-express": "^0.47.0",
    "@opentelemetry/instrumentation-http": "^0.57.0",
    "@opentelemetry/sdk-node": "^0.57.0",
    "@opentelemetry/sdk-trace-base": "^1.30.0",
    "@xmldom/xmldom": "^0.7.13",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.22.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^6.2.0",
    "express": "^4.21.2",
    "https-utils": "^2.0.1",
    "jest": "^29.7.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.21",
    "mysql2": "^3.11.5",
    "object-hash": "^3.0.0",
    "product-id-parser": "^3.0.1",
    "routing-controllers": "^0.10.4",
    "routing-controllers-openapi": "^4.0.0",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^4.6.3",
    "trace-logger": "^3.1.3",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20",
    "xpath": "0.0.32"
  },
  "devDependencies": {
    "@types/cls-hooked": "^4.3.9",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/google-protobuf": "^3.15.12",
    "@types/lodash": "^4.14.199",
    "@types/mocha": "^10.0.2",
    "@types/node": "^22.10.2",
    "@types/object-hash": "^3.0.4",
    "@types/supertest": "^2.0.14",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "dsbs-prettier-configuration": "^1.0.0",
    "eslint": "^8.51.0",
    "eslint-config-dsbs": "^1.0.6",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "reflect-metadata": "^0.2.2",
    "supertest": "^3.4.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-sinon": "1.0.24",
    "typescript": "^5.7.2"
  }
}
